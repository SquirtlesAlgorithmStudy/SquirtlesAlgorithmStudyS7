    "from collections import deque\n",
    "\n",
    "queue = deque()\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "for i in range(N):\n",
    "    queue.append(i+1)\n",
    "\n",
    "while len(queue) != 1:\n",
    "    queue.popleft()\n",
    "    queue.append(queue[0])\n",
    "    queue.popleft()\n",
    "    \n",
    "print(queue[0])\n"
   ]
  }
